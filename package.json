{
  "name": "simde",
  "version": "2.2.0",
  "description": "Educational superescalar and VLIW simulator",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "nyc ava",
    "docs": "typedoc src/index.ts --excludePrivate --mode file --theme minimal --out build/docs && opn build/docs/index.html",
    "docs:json": "typedoc --mode file --json build/docs/typedoc.json src/index.ts",
    "cov": "yarn unit && yarn html-coverage && opn coverage/index.html",
    "check-coverage": "nyc check-coverage --lines 70 --functions 70 --branches 70",
    "html-coverage": "nyc report --reporter=html",
    "send-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etsiiull/SIMDE.git"
  },
  "author": "Adrian Abreu Gonzalez",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/etsiiull/SIMDE/issues"
  },
  "homepage": "https://github.com/etsiiull/SIMDE",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "bootstrap": "^5.1.3",
    "font-awesome": "4.7.0",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "^2.2.0",
    "i18next-localstorage-cache": "1.1.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-color": "^2.14.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.0",
    "react-router": "^6.0.0",
    "react-router-dom": "^6.0.0",
    "redux": "^4.2.0",
    "tslib": "^2.2.0",
    "typescript-parsec": "^0.3.4"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "ava": "^5.0.0",
    "codecov": "^3.6.5",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "multiview": "3.0.1",
    "nyc": "15.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-scss": "^5.3.0",
    "typedoc": "^0.25.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "ava": {
    "files": [
      "build/module/test/**/*.spec.js",
      "build/module/test/**/*.spec.js"
    ]
  }
}
